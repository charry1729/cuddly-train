# CineStox Backend Environment Configuration
# Copy this file to .env and fill in your actual values

# App Configuration
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=your-super-secret-key-change-in-production

# Database Configuration
DATABASE_URL=postgresql://cinestox:password@localhost:5432/cinestox

# Redis Configuration
REDIS_URL=redis://localhost:6379

# External API Keys
TMDB_API_KEY=your_tmdb_api_key_here
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here

# Google Cloud Configuration (for Telugu voice commands)
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_CLOUD_CREDENTIALS=path_to_your_credentials.json

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200

# Trading Engine Configuration
INITIAL_BALANCE=10000.0
MAX_LEVERAGE=5.0
LIQUIDATION_THRESHOLD=0.1

# Event Configuration
FDFS_HYPE_RADIUS_KM=50.0
SANKRANTHI_BATTLE_DURATION_DAYS=3

# NFT Configuration
NFT_MINTING_FEE=100.0
MAX_NFT_SUPPLY=1000

# Performance Configuration
MAX_CONCURRENT_TRADES=1000
WEBSOCKET_HEARTBEAT_INTERVAL=30 